#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/anonymous/Documents/catpumajs/node_modules/.pnpm/mathjs@14.0.0/node_modules/mathjs/bin/node_modules:/home/anonymous/Documents/catpumajs/node_modules/.pnpm/mathjs@14.0.0/node_modules/mathjs/node_modules:/home/anonymous/Documents/catpumajs/node_modules/.pnpm/mathjs@14.0.0/node_modules:/home/anonymous/Documents/catpumajs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/anonymous/Documents/catpumajs/node_modules/.pnpm/mathjs@14.0.0/node_modules/mathjs/bin/node_modules:/home/anonymous/Documents/catpumajs/node_modules/.pnpm/mathjs@14.0.0/node_modules/mathjs/node_modules:/home/anonymous/Documents/catpumajs/node_modules/.pnpm/mathjs@14.0.0/node_modules:/home/anonymous/Documents/catpumajs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/cli.js" "$@"
else
  exec node  "$basedir/../../bin/cli.js" "$@"
fi
